{
  "manifest_version": 2,
  "name": "GitHub2Jira",
  "description": "Go to jira ticket from github",
  "version": "1.0.1",
  "icons": {
    "16": "icon16.png", 
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "options_page": "options.html",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["https://github.com/*/*"],
      "js": ["contentscript.js"],
      "css": ["contentscript.css"]
    }
  ]
}